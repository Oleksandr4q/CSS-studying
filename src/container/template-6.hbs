
    <header class='header'>
		<div>
			{{> logo}}
		</div>

      		<nav class='header_nav'>
				<div class="header_nav_right">
				{{>nav list=header}}
				</div>
	  		</nav>
    </header>

	<main class="main">

		{{#with info}}
		<div class="info">
			<h1 class="info_title">{{title}}</h1>
			<p class="info_description">{{description}}</p>
		</div>	
		{{/with}}

		{{#with form}}
			<form class="form">

				<div class="form_input_list">
					{{#each inputs}}
						<input type="{{type}}" placeholder="{{placeholder}}" class="form_input" name="{{name}}" id="form-input-{{id}}" />
					{{/each}}
				</div>

				<div class="form_checkbox">
					<input type="checkbox" name="{{checkbox.name}}" id="{{checkbox.name}}">
					<div></div>
					<label for="{{checkbox.name}}">{{checkbox.label}}</label>
				</div>

				<button type="submit" class="form_button">{{button}}</button>
				<p class="form_info">{{info}}</p>
			</form>
		{{/with}}
	</main>

	<div class="feedback">
		{{#with updates}}
			<div class="updates">
				<h1 class="updates_title">{{title}}</h1>

		{{#each list}}
			<div class="msg">
				<strong class="msg_name">{{nickname}}</strong>
				<p class="msg_text">{{description}}</p>
			</div>
		{{/each}}
			<a href="url" class="messages_link">All updates</a>
			</div>
		{{/with}}
	</div>

	<div class="box">
			{{#with expirience}}
				<h2 class="expirience_title">{{title}}</h2>
					<div class="expirience">
				{{#each list}}
				<div>
					<h3>{{name}}</h3>
					<p>{{details}}</p>
				</div>
				{{/each}}	
					</div>
			{{/with}}
	</div>

	<footer class="page_block">	
	<div class="footer">
		<div>
			{{> logo}}
			<span class="footer_copyright">Copyright 2023</span>
		</div>

		{{#each footer}}
			<div>
				<span class="footer_title">{{title}}</span>
				{{>nav list=links isVertical=true}}
			</div>
		{{/each}}
	</div>
	</footer>